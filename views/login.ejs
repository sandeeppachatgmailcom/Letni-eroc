  <form id="loginform" action="/authenticate/login" method="post">

    <div class="LoginPage d-flex justify-content-evenly" data-ride="carousel"
      style="background-position: center ;   background-image: url('/Images/1694680589320-image (2).jpg'  );  
      ">
      <div class=" container-fluid" style="background-color: rgba(62, 62, 62, 0.668);">
        

      <ol class="carousel-indicators">
        <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
      </ol>
      <div class="login col-3">
        <div class="AlignCenter  ">

          <h1 class=" "> Welcome </h1>

          <label class="Login_Test" for="login_DIv1_Username">Usename</label>
          <input class="col-12  btn " onchange="CheckUser()" style="border-radius: 10px;" id="login_DIv1_Username"
            title="Username is case sencitive" type="Login_Test" name="Username" placeholder="Usename">
          <select id="idCompanyList" style="text-transform:uppercase" class="form-control mt-3">


          </select>



          <label class="Login_Test" for="login_DIv1_Password">Password</label>
          <input class="Login_Test form-control btn" id="login_DIv1_Password" type="password"
            title="Password is case sencitive" name="Password" placeholder="password">
            <div class="container-fluid">
              <button class="btn btn-success col-3" type="submit" value="">  SIGN IN </button>
              <a style="text-decoration: none "class="btn col-3 form-control btn-primary AlignCenter bi bi-person-plus-fill" href="/authenticate/signup">SIGN UP</a>
              
            </div>

           


        </div>
      </div>
      </div>
    </div>
  </form>


  <script src="/Script/Script_Signup.js"> </script>
