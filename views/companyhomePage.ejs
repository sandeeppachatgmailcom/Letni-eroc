<%- include('header') %>
<%- include('MenuBarCustomer') %>
<div class=" container-fluid">
     
    <div class="container-fluid" >
        <div class="container-fluid " >
          <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
            <div class="carousel-inner">
              <div class="carousel-item active">
                <img class="d-block w-100 " style="position: relative; height:500px ;  background-size: cover; background-position: center; overflow: hidden;"  src="<%=profile.image1%>" alt="First slide">
              </div>
              <div class="carousel-item">
                <img class="d-block w-100" style="position: relative; height: 500px; background-size: cover; background-position: center; overflow: hidden;" src="<%=profile.image2%>" alt="Second slide">
              </div>
            
            </div>
            <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
              <span class="carousel-control-prev-icon" aria-hidden="true"></span>
              <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
              <span class="carousel-control-next-icon" aria-hidden="true"></span>
              <span class="sr-only">Next</span>
            </a>
          </div>
        </div>
    </div>
    <div class="container-fluid">
        
        <div class="container-fluid col-8 " style="background-color: rgba(48, 41, 64, 0.701);" >
          <div class="container-fluid " style="background-color: rgba(41, 64, 41, 0.701) ; flex-wrap: wrap ;">
           
            <% tariffpackages.forEach(element => { %>

              <div class="card text-white bg-secondary mb-3" style="max-width: 18rem;">
                <div class="card-header"><%= element.tariffName %></div>
                <div class="card-body">
                </div>
                <button class="btn btn-light" value="<%= element.tariffIndex %>" ><%= element.tariffName %></button>
              </div>
             
            <% }) %>
          
           
         



          </div>
        </div>
        <div class="container-fluid col-2" style="background-color: rgba(64, 41, 50, 0.703);">
       
        </div>
    </div>
</div>

<%- include('footer') %>