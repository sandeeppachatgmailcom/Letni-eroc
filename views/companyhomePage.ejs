                                    <% } else { %>
                                    <button type="button" id="idSwitchRoomsStatus<%= items.roomIndex%>" onclick="switchRoomStatus(event.target.value,'idSwitchRoomsStatus<%= items.roomIndex%>')" value="<%= items.roomIndex%>"  class="btn btn-sm btn-danger bi bi-power col-4">OFF </button>
                                  <% } %>
                                  
                                </td>
                              </tr>
                              <% }) %>
                              
                    
                    
                        </tbody>
                      </table>
                    </div>
                  </div>
              <% } %>

              <% if ( reservation ) { %>
                <div id="idReservations" class="  container-fluid border border-primary rounded  p-1 m-2" style=" height: 250px; overflow: hidden ;">
