                    <table class="table">
                      <button onclick="collapse('idReservations', 250 )" class="bi bi-caret-down-square  btn btn-primary col-3"> Reservations   </button> 
                      <tbody>
                      
                          
                          <% reservation.forEach(items=> { %>
                            <tr>
                              <td class="">
                                <small class="font-weight-bold mt-1  ">
                                  <%= items.reservationNumber %> <br>
                                </small>
                                  <small class="font-weight-bold mt-1"><%= items.tariffName %>  </small>
                                
                              </td>
                              <td>
                                
                                <small class="font-weight-bold  mt-1"> <%= items.firstName %> </small> <br>
                                <small class="   mt-1"> <%= items.email %> </small> <br>
                                <small class="  mt-1"> <%= items.contactNumber %> </small> <br>
                                <small class="  mt-1"> <%= items.preferance %> </small> <br>
                                
                                </td> 
                              <td>
                                <button class="btn  btn-primary bi bi-box-arrow-in-down-right " > </button> <small class="font-weight-bold text-primary  mt-1 ">  ARR <%=  (items.arrivalDate).toString().slice(0,24) %>  </small> <br>
                                <button class="btn  btn-warning bi bi-box-arrow-up-left" > </button> <small class="font-weight-bold text-warning  mt-1"> DEP <%=  (items.depart_Date).toString().slice(0,24) %>  </small> <br>
                                <button class="btn  btn-info bi bi-house-door" > </button> <small class="font-weight-bold text-secondary  mt-1"> ROOMS <%= items.totalRoom %>  </small> <br> 
                                <button class="btn  btn-secondary bi bi-people-fill" > </button> <small class="font-weight-bold text-secondary  mt-1"> GUEST <%= items.TotalPax %></small> 
                                
                                
                              </td>
                              <td>
                                <button type="button" class="btn btn-sm btn-danger bi bi-power">VIEW</button>
                  
                              </td>
                            </tr>
                            <% }) %>
                            
                  
                  
                      </tbody>
                    </table>
                  </div>
                </div>
            <% } %>
            <% if ( payments ) { %>
              <div id="idPayments" class="  container-fluid border border-primary rounded  p-1 m-2" style=" height: 250px; overflow: hidden ;">
                <div class="table-responsive bg-white border-primary " >
                  <table class="table">
                    <button onclick="collapse('idPayments', 250 )" class="bi bi-caret-down-square  btn btn-primary col-3"> Collection </button>
                     <tbody>
                      <tr class="border border-primary text-primary">
                        <td class="">
                          <small class="font-weight-bold mt-1  ">
                            VOUCHER
                          </small>
                        </td>
                        <td class="">
                          <small class="font-weight-bold  mt-1">  ENTRY DETAILS </small> <br>
                        </td>
                        <td>
                          <small class="font-weight-bold  mt-1">  CREDIT  </small>  
                          
                        </td>
                        <td>
                          <small class="font-weight-bold  mt-1">  DEBIT </small>  
            
                        </td>
                      </tr>
                        <% payments.forEach(items=> { %>
                          <tr>
                            <td class="">
                              <small class="font-weight-bold mt-1  ">
                                <%= items.paymentIndex %>
                              </small>
                            </td>
                            <td class="">
                              <small class="font-weight-bold  mt-1">  <%= items.paymentReferance %> </small> <br>
                              <small class="font-weight-bold  mt-1">  <%= items.customerName %> </small> <br>
                              <small class="font-weight-bold  mt-1">  <%= items.transactionReferanceNumber %> </small> <br>
                              
                            </td>
                            <td>
                              <small class="font-weight-bold  mt-1">  <%= items.debit %> </small>  
                              
                            </td>
                            <td>
                              <small class="font-weight-bold  mt-1">  <%= items.credit %> </small>  
                
                            </td>
                          </tr>
                        <% }) %>
                    </tbody>
                  </table>
                </div>
              </div>
          <% } %>
            </div>
        </div>
        
      </div>
  </div>
