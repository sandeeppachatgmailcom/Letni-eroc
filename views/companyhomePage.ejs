<%- include('header') %>
<%- include('MenuBarCustomer') %>
<div class="container-fluid justify-content-center row w-100"> 
    <div class="container-fluid   w-100 btn  row d-flex bg-secondary d-flex flex-row"   style="background-size: cover; background-position: center; background-image: url('<%= profile.image1 %>'); height: 400px;">
        <div class="container-fluid col-9">

        </div>
      <canvas id="eventChart" style="display: block; height: 238px; width: 476px;" width="952" height="476" class="chartjs-render-monitor"></canvas>
    </div>
    <div class="container-fluid d-flex flex-row"  >
      <div class="container-fluid col-2">
        <small style="font-size: 50px;" class="text-secondary "> <%= profile.lastName %></small>  
      </div>
      <div class="container-fluid ">
        <small class="text-secondary">Email: <%= profile.email %></small>  
      <small class="text-secondary">Contact: <%= profile.contactNumber %></small> <br>
      <small class="text-secondary">Address: <%= profile.StreetName %> <%= profile.city %> pin:<%= profile.pincode %></small>  
      
      </div>
  </div>
  
    <div class="container-fluid d-flex ">
        <div class=" col-2 container-fluid border rounded  ">
          <form action="/vedurehomepage/loadhomepage" method="post">
            <button class="btn border btn-success col-12 col "> Dashboard </button>
          </form>
          <form action="/Company/loadTariff" method="get">
              <button class="btn border btn-success col-12 col "> Tariff </button>
          </form>
          <form action="/Company/loadPlan" method="get">
              <button class="btn border btn-success col-12 col "> Checkin Plan </button>
          </form>
          <form action="/Company/loadRoom" method="get">
              <button class="btn border btn-success col-12 col "> Rooms </button>
          </form>
          <form action="/Company/loadReservations" method="get">
              <button class="btn border btn-success col-12 col "> Reservation Details </button>
          </form>
          <form action="/Company/loadPayments" method="get">
              <button class="btn border btn-success col-12 col "> Collection Details </button>
          </form>
          <form action="/Company/loadGallery" method="get">
              <button class="btn border btn-success col-12 col "> Gallery </button>
          </form>
          </div>
          <div class="col-10 container-fluid d-flex border rounded"  >
            <div class="col-6 container-fluid "  >
              <% if ( occupancyDetails ) { %>
                <div  class="col-12 container-fluid border rounded  border-primary p-1 m-2" >
                  <div class="table-responsive  border-primary  " >
                    <table class="table border-primary">
                      <button  class="bi bi-caret-down-square  btn btn-primary col-6"> ROOM ALIGATION</button>
                       <tbody>
                          <% occupancyDetails.forEach(occupancy=> { %>
                            <tr class="text-secondary">
                              <td > 
                                <small class="font-weight-bold mt-1"> <%= occupancy.reservationNumber %> </small> <br>
                                <small class="font-weight-bold mt-1"> <%= occupancy.customer.firstName %> </small> <br>
                                <small class="font-weight-bold mt-1"> <%= occupancy.customer.contactNumber %> </small> <br>
                                <small class="font-weight-bold mt-1"> <%= occupancy.customer.email %> </small> <br>
                                <small class="font-weight-bold mt-1"> <%= occupancy.customer.pincode %> </small> <br>
                              </td>
                              <td > 
                                <small class="font-weight-bold  mt-1">  <%= occupancy.tariff.tariffName %>  </small> <br>
                                <small class="font-weight-bold  mt-1">  Rent :<%= occupancy.bookings.specialRate %> Extra Person <%= occupancy.bookings.ExtraChargeRent %>   </small> <br>
                                <% if (occupancy.plan) { %>
                                  <small class="font-weight-bold  mt-1">  <%= occupancy.plan.planName %>  </small> <br> 
                                  <small class="font-weight-bold  mt-1">  Checkin plan :<%= occupancy.bookings.PlanAmount %> Extra person plan <%= occupancy.bookings.ExtraChargeRent %>   </small> <br>
                                  <% } %>
                                <small class="font-weight-bold  mt-1">  Arrival <%= (occupancy.bookings.arrivalDate).toString().slice(0,21) %>  </small> <br>
                                <small class="font-weight-bold  mt-1">  Departure <%= (occupancy.bookings.depart_Date).toString().slice(0,21) %>  </small> <br>
                                <small class="font-weight-bold  mt-1">  <%= occupancy.bookings.specialrequierments %>  </small> <br>
                                </td>
                              <td > 
                                <small class="font-weight-bold text-success  mt-1">  </small>
                              </td>
                              <td >
                                                                 
                              </td>
                              <td >
                                <% if (occupancy.room) { %>
                                  <div class="container-fluid d-flex"> 
                                    <button class="btn btn-warning col-6 bi bi-journal-bookmark-fill "> <%= occupancy.room.roomName %> </button>
                                    <button class="btn btn-danger col-6 bi bi-trash3 "> Unlink  </button>
                                  
                                  </div>
                                  <% } else { %>
                                    <button type="button " onclick="loadAvailableRooms(event.target.value)" value="<%= JSON.stringify(occupancy) %>"  class="btn btn-sm btn-success bi bi-power col-6"> SHOW ROOMS </button>
                                    <button type="button " onclick="updateReservationWithRoom(event.target.value)" value="<%=JSON.stringify(occupancy) %>"  class="btn btn-sm btn-success bi bi-power col-5 "> SAVE</button>  
                                    <select  class="btn col-12"  name="nameAvailableRoom" id="idAvailableRoom<%= occupancy.occupancyIndex %>" >
                                      <option  value="">  </option> 
                                      <%= console.log(occupancy.room,'occupancy.roomoccupancy.roomoccupancy.roomoccupancy.room') %>
                                          <option  value="">  </option> 
                                      </select> 
                                
                                <% } %>
                                
                                    </td>
                                  
                            </tr>
                            <% }) %>
                          </tbody>
                    </table>
                  </div>
                </div>
                <% } %>
            </div>  
            <div class="col-6 container-fluid "  >
                <% if ( Plans ) { %>
                <div id="idDivPlans" class="col-12 container-fluid border rounded  border-primary p-1 m-2" style=" height: 250px; overflow: hidden ;">
                  <div class="table-responsive  border-primary  " >
                    <table class="table border-primary">
                      <button onclick="collapse('idDivPlans', 250 )" class="bi bi-caret-down-square  btn btn-primary col-3"> Checkin Plans</button>
                      <button type="button " data-bs-toggle="modal" data-bs-target="#idModalnewCheckinPlan"
                      class="btn btn-success h-100 btn bi bi-clipboard-plus  text-light "> CREATE  </button>
                      <tbody>
                       
                          <% Plans.forEach(plans=> { %>
                            <tr>
                              <td class="col-3">
                                <small class="font-weight-bold mt-1  ">
                                  <%= plans.planName %>
                                </small>
                              </td>
                              <td class="col-2">
                                <small class="font-weight-bold  mt-1"> &#8377 <%= plans.amount %>(<%= plans.maxPax %>) </small>
                              </td>
                              <td class="col-2">
                                <small class="font-weight-bold  mt-1"> Extra &#8377 <%= plans.extraCharge %>/- </small>
                                
                              </td>
                              <td class="col-4">
                                <div class="container-fluid">
                                  <button data-bs-toggle="modal" data-bs-target="#idModalnewCheckinPlan<%=plans.planIndex%>"  type="button" class="col-5 btn  btn-warning bi bi-pencil-fill">EDIT</button>
                                  <% if (plans.isActive) { %>
                                    <button type="button" value="<%=plans.planIndex%>,<%=profile.CompanyID%>"
                                      onclick="enableplanModal(event.target.value)"
                                      class="btn bi bi-power col-6 btn-danger "> OFF </button>
                                    <% } else { %>
                                      <button type="button" value="<%=plans.planIndex%>,<%=profile.CompanyID%>"
                                        onclick="enableplanModal(event.target.value)"
                                        class="btn bi bi-power col-6  btn-success"> ON </button>
                                      <% } %>
                                </div>
                  
                              </td>
                            </tr>
                            <% }) %>
                              
                  
                  
                      </tbody>
                    </table>
                  </div>
                </div>
                <% } %>
                <% if ( tariffPackages ) { %>
                  <div id="idTariffDetails" class="  container-fluid border border-primary rounded p-2  m-2"style="height:250px;   overflow: hidden ;" >
                  <div class="table-responsive bg-white border-primary " >
                    <table class="table">
                      <button onclick="collapse('idTariffDetails', 250 )" class="bi bi-caret-down-square  btn btn-primary col-3"> Classification</button onclick="">
                        <button class="btn btn-success  bi bi-clipboard-plus" data-bs-toggle="modal"  data-bs-target="#idModalnewTariff"> CREATE  </button>   
                        <tbody>
                      
                            <% tariffPackages.forEach(tariff => { %>
                              <tr >
                                  <td class="">
                                      <small class="font-weight-bold mt-1  "> <%= tariff.tariffName %> </small>
                                </td>
                                <td class="">
                                  <small class="font-weight-bold  mt-1"> &#8377 <%= tariff.SpecialRent %>(2)  </small>
                                </td>
                                <td>
                                  <small class="font-weight-bold  mt-1">Extra &#8377 <%= tariff.extraPerson %>/-  </small>
                                    
                                  </td>
                      
                                <td>
                                  <button class="btn btn-warning bi bi-pencil-fill" data-bs-toggle="modal"  data-bs-target="#<%= tariff.tariffName%>"value="<%= tariff.tariffIndex %>"   >EDIT</button>
                                  
                       
                                  <% if (!tariff.isActive) { %>
                                    <button id="'btn'+<%= tariff.tariffname %>"value="<%= tariff.tariffIndex%>,<%= profile.CompanyID%>"class="btn btn-success m-1 bi bi bi-power" type="button" onclick="enableTariff(event.target.value)"> ON </button>
                                <% } else { %>
                                    <button id="'btn'+<%= tariff.tariffname %>" value="<%= tariff.tariffIndex%>,<%= profile.CompanyID%>" class="btn btn-danger m-1 bi bi-power" type="button" onclick="freezeTariff(event.target.value)">OFF </button>
                                <% } %>
                                </td>
                              </tr>
                            <% }) %>
                      
                        
                        
                      </tbody>
                    </table>
                  </div>
                  </div>
                <% } %>

